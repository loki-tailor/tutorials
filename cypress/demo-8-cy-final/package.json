		"@typescript-eslint/parser": "5.x",
		"cucumber-json-report-formatter": "0.1.x",
		"cypress": "10.x",
		"esbuild": "0.15.x",
		"eslint": "8.x",
		"eslint-config-prettier": "8.x",
		"eslint-plugin-no-loops": "0.3.x",
		"eslint-plugin-prettier": "4.x",
