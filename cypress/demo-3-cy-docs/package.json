{
  "name": "demo-3-cy-docs",
  "version": "0.1.0",
  "description": "demos from cypress.io documentation",
  "main": "index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/loki-tailor/tutorials.git"
  },
  "config": {
    "$comment": "system level environment variables go here..."
  },
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "cross-env-data": "cross-env CYPRESS_VERIFY_TIMEOUT=6",
    "envPath": "echo %PATH%",
    "$-1---------": "comment_---------------------------------------------------------",
    "cy:open": "cypress open",
    "y:cy:open": "yarn cypress open",
    "cy:debug:open": "set DEBUG=cypress:* & cypress open",
    "cy:debug-launcher:open": "set DEBUG=cypress:launcher & cypress open",
    "cy:debug-ser-proj:open": "set DEBUG=cypress:server:project & cypress open",
    "cy:debug-custom:open": "set DEBUG=cypress:cli,cypress:server:*,cypress:launcher,cypress:network:*,cypress:net-stubbing*,cypress:webpack,-cypress:server:util:process_profiler & cypress open",
    "$-2---------": "comment_---------------------------------------------------------",
    "cy:verify": "cypress verify",
    "y:cy:verify": "yarn cypress verify",
    "cy:debug-cli:verify": "set DEBUG=cypress:cli & cypress verify",
    "$-3---------": "comment_---------------------------------------------------------",
    "ts:run-one": "ts-node run_one.ts",
    "cy:debug-cli:run": "set DEBUG=cypress:cli & cypress run",
    "y:cy:run:show-envs": "yarn cypress run --spec cypress/e2e/env_print.cy.js",
    "cy:test-one": "cypress run --spec cypress/e2e/1-getting-started/todo.cy.js",
    "$-4---------": "comment_---------------------------------------------------------",
    "cy:setEnv-test-1": "set CYPRESS_cliArgUserName=lokeshwar & cypress open",
    "cy:setEnv-test-2": "as-a global npm run cy:open",
    "cy:setEnv-test-3": "sys_env.bat && cypress run",
    "cy:setEnv-test-4": "sys_env.bat && npx cypress verify",
    "cy:setEnv-test-5": "sys_env.bat && npx cypress open",
    "cy:setEnv-test-6": "set CYPRESS_VERIFY_TIMEOUT=60000 & npx cypress verify",
    "cy:setEnv-test-7": "npm run cross-env-data npm run cy:verify",
    "$-5---------": "comment_---------------------------------------------------------",
    "setEnv": "sys_env.bat",
    "resetEnv": "set CYPRESS_VERIFY_TIMEOUT=",
    "cy:cvt": "echo %CYPRESS_VERIFY_TIMEOUT%",
    "testA": "echo \"Error: no test specified\"",
    "testB": "echo \"Error: no test specified B\"",
    "$-6---------": "comment_---------------------------------------------------------",
    "testC": "ts-node ./js-practice/test.js",
    "$-7---------": "comment_---------------------------------------------------------"
  },
  "author": "lokeshwar",
  "license": "MIT",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "as-a": "^2.2.1",
    "browserlist": "^1.0.1",
    "cross-env": "^7.0.3",
    "cypress": "10.x",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.x",
    "typescript": "*"
  }
}
